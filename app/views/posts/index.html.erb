


<div class = "container d-flex col-sm justify-content-center">
    <div></div>
    <% if user_signed_in? %>
    <div class="main-container"> 
        <%=image_tag(current_user.avatar_url) if current_user.avatar? %>
        <div class="align-item-center">
            <%=render 'form' %>
        </div>
        <div>
            <%=render 'feed' %>
        </div>
    </div>
    
    <div>
        <%= render 'friendreq'%>
    </div>
    <%else%>
        <h2> Welcome to Fakebook! </h2>
    <%end%>
</div>
